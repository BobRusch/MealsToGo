{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es2021": true
  },
  "extends": [
    "react-app",
    "plugin:react/recommended",
    "standard",
    "@react-native-community"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "react/prop-types": ["off"],
    "react/style-prop-object": ["off"],
    "camelcase": [0, { "properties": "never" }],
    "quotes": [2, "double", { "avoidEscape": true }]
  }
}
